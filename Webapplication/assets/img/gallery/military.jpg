var lastScrollTop = 0;
function do_the_header(){
  var st = $(this).scrollTop();
  if (st > lastScrollTop){
      // downscroll code
      $('body').removeClass('scrolled-up').addClass('scrolled-down');
  } else {
     // upscroll code
     $('body').removeClass('scrolled-down').addClass('scrolled-up');
  }
  lastScrollTop = st;
  if($(document).scrollTop()>$('page-header').height()){
    $('body').addClass('has-scrolled');
    // should close page-header's drop down menu
  } else {
    $('body').removeClass('has-scrolled');
    $('top-header .navbar-toggler[aria-expanded="true"]').trigger("click");
    // should close top-header's drop down menu
  }
}
function bigtext_with_limits(){
  $("[class*='-bigtext-']").each(function(){
    var limits = $(this).attr('class').match(/(\d+)-bigtext-(\d+)/);
    if(limits){
      $(this).bt({
        minfontsize: limits[1],
        maxfontsize: limits[2]
      });
      console.log("bigtext_with_limits", limits[1], limits[2]);
    }
  });
}
jQuery(window).on('scroll',function(){do_the_header();});
$("[data-scrollto]").click(function(event) {
  event.preventDefault();
  var mysection=$(this).attr('data-scrollto');
    $('html, body').animate({
        scrollTop: $("#"+mysection).offset().top
    }, 500);
});

$(document).ready(function(){
  console.log("ready.");

  $('.bigtext').not("[class*='bigtext-']").bt();
  bigtext_with_limits();

  $(window).resize(function(e){
    $('.bigtext').not("[class*='bigtext-']").bt();
    bigtext_with_limits();
  });
  $('.article-preview').on('click tap', function (e) {
    $('.article-preview').removeClass('active');
    $(this).addClass('active');
  });
  $(document).on('click', '.more-posts-please', function (e) {
    console.log("more posts please");
    // Don't follow the link
    e.preventDefault();

    // Create a loading message and add it in place of the link
    loadingMessage = $('<div class="col more-posts-loading d-flex justify-content-center"><div class="align-self-center">Loading...<i class="fa fa-spinner fa-spin"></i></div></div>');
    $('.more-posts-please').replaceWith(loadingMessage);
    console.log("before appendto:"+lastScrollTop);
    // Fetch the next page
    $.get($(this).attr('href'), function(data)
    {
      // Only grab the part of the page we want
      content = $(data).find('.ajax-wrapper').children().hide();

      // Add it to the DOM
      $(content).appendTo('.ajax-wrapper');

      // Remove loading indicator
      $('.more-posts-loading').fadeOut('fast').remove();

      // Fade-in our new content
      $(content).fadeIn('fast');
      $(this).scrollTop(lastScrollTop);
      console.log("after appendto:"+lastScrollTop);
    });
  });
});
jQuery(window).on("load", function () {
  $('.bigtext').not("[class*='bigtext-']").bt();
  bigtext_with_limits();
});
var bt = BigText.noConflict(true);
$.fn.bt = bt.jQueryMethod;
